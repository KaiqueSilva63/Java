import java.util.Scanner;
/*Conversor de temperatura de fahreit para graus Celsius*/
public class Main{
	public static void Exercicio7(String[] args) {
	    Scanner scanner = new Scanner(System.in);
	    double F = scanner.nextDouble();
		double C = 5.0*(F - 32.0)/9.0;
		System.out.println(String.format("A temperatura em Celsius Ã©: " + "A=%.3f",C));
	}
}
